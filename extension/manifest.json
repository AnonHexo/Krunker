{
   "name":"Krunker Hacks",
   "version":"2.1.2",
   "manifest_version":2,
   "homepage_url":"https://github.com/AnonHexo",
   "description":"Private Krunker Script. [by skidlamer, chomler]",
   "browser_action":{
      "default_icon":"main/icons/icon.png",
      "default_title":"Krunker Hack."
   },
   "permissions":[
      "webRequest",
      "webRequestBlocking",
      "https://krunker.io/*",
      "https://assets.krunker.io/*"
   ],
   "web_accessible_resources":[
      "*.js",
      "*.css",
      "*.obj",
      "*.png",
      "*.mp3",
      "*.ttf",
      "*.txt",
      "*.html",
      "*.json"
   ],
   "background":{
      "scripts":[
         "main/main.js"
      ]
   }
}
